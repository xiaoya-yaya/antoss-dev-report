{
  "name": "2025-ant-developer-oss-report-h5",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://antoss-dev-report.open-digger.cn",
  "repository": {
    "type": "git",
    "url": "https://github.com/antgroup/antoss-dev-report.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "predeploy": "npm run build",
    "deploy": "BASE_PATH=/2025-ant-developer-oss-report-h5/ npm run predeploy && gh-pages -d dist",
    "dev": "vite",
    "lint": "eslint . && stylelint \"**/*.{css,less,scss}\"",
    "lint:fix": "prettier --write . && eslint --fix . && stylelint --fix \"**/*.{css,less,scss}\"",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "commitlint": {
    "extends": [
      "ali"
    ]
  },
  "lint-staged": {
    "*.{cjs,cts,js,jsx,mjs,mts,ts,tsx,vue}": "eslint --fix",
    "*.{css,less,scss}": "stylelint --fix",
    "*.{cjs,css,cts,html,js,json,jsx,less,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}": "prettier --write"
  },
  "prettier": "prettier-config-ali",
  "eslintConfig": {
    "extends": [
      "ali/typescript/react",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off"
    },
    "ignorePatterns": [
      "scripts/*"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-ali",
      "stylelint-prettier/recommended"
    ]
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@react-spring/web": "^9.7.5",
    "canvas-confetti": "^1.9.4",
    "constate": "^3.3.2",
    "gsap": "^3.14.1",
    "html2canvas": "^1.4.1",
    "motion": "^12.23.26",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-fast-marquee": "^1.6.5",
    "swiper": "^11.1.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@eslint/js": "^9.11.1",
    "@rollup/plugin-dsv": "^3.0.5",
    "@types/canvas-confetti": "^1.9.0",
    "@types/postcss-pxtorem": "^6.1.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "commitlint-config-ali": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-ali": "^15.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "fontmin": "^2.0.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.3.3",
    "prettier-config-ali": "^1.3.2",
    "sass-embedded": "^1.79.4",
    "stylelint": "^16.10.0",
    "stylelint-config-ali": "^2.1.1",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
